{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "1.0.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "jsw_tool (convert, mm)",
      "cwd": "${workspaceFolder}",
      "cargo": {
        "args": ["build", "--manifest-path", "${workspaceFolder}/rust/jsw_tool/Cargo.toml"]
      },
      "args": ["convert", "./resources/mm/bin/mm.bin", "./resources/mm/gameo/"],
      "sourceLanguages": ["rust"]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Rust (current project)",
      "cargo": {
        "args": ["build", "--manifest-path", "${fileDirname}/../Cargo.toml"]
      },
      "sourceLanguages": ["rust"]
    }
  ]
}
